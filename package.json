{
  "name": "company",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "frontend-crm:start": "nx serve frontend-crm",
    "frontend-crm:build": "nx build frontend-crm",
    "frontend-crm:test": "nx test frontend-crm",
    "frontend-admin:start": "nx serve frontend-admin",
    "frontend-admin:build": "nx build frontend-admin",
    "frontend-admin:test": "nx test frontend-admin",
    "backend-api:start": "nx serve backend-api",
    "backend-api:build": "nx build backend-api",
    "backend-api:test": "nx test backend-api",
    "backend-api:compodoc": "mkdir docs-temp && cp README.md ./docs-temp && cp ./apps/backend/api/docs/README.md ./ && npx compodoc -p apps/backend/api/tsconfig.app.json --theme vagrant -r 8085 -d docs/api/ && rm README.md && cp ./docs-temp/README.md ./ && rm ./docs-temp/README.md && rmdir ./docs-temp",
    "db:entities": "node_modules/.bin/tsc --project apps/backend/api/tsconfig.entities.json",
    "db:migrations:run": "node_modules/.bin/tsc --project apps/backend/api/tsconfig.migrations.json && node_modules/.bin/ts-node node_modules/.bin/typeorm migration:run -t=false -d ormconfig",
    "db:migrations:revert": "node_modules/.bin/ts-node node_modules/.bin/typeorm migration:revert",
    "db:migration:create": "node_modules/.bin/ts-node node_modules/.bin/typeorm migration:create apps/backend/api/src/migrations/user",
    "db:start": "cp .env ./docker-compose && cd docker-compose && docker-compose up -d",
    "db:stop": "cd docker-compose && docker-compose stop && rm .env",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test"
  },
  "private": true,
